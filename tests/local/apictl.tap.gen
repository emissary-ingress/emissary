#!/usr/bin/env bash
set -o pipefail

check_version() {
	local out
	out=$(./bin_${GOHOSTOS}_${GOHOSTARCH}/apictl version 2>&1)
	[[ $out == 0.* ]]
}

######################################################################

n=0
do_test() {
	local name=$1
	shift
	n=$((n+1))
	if "$@" | sed 's/%/# /'; then
		echo "ok $n $name"
	else
		echo "not ok $n $name"
	fi
}
echo 'TAP version 13'
echo 1..1
do_test check_version
