#!/usr/bin/env bash

if ! type docker &>/dev/null; then
	exit 77
fi

virtualenv $(pwd)/venv --python python3
$(pwd)/venv/bin/pip install git+https://github.com/tradel/consul-kube

export PATH=$(pwd)/venv/bin:$PATH

echo "===PATH==="
echo $PATH

echo "===consul-kube==="
which consul-kube

go test -tags=test -v ./tests/cluster/consul
