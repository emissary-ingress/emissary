#!/usr/bin/env bash

cd "${BASH_SOURCE[0]%/*}/../.." &&
set -a && . ./k8s-env.sh &&
cd tests/cluster/oauth-e2e &&
./node_modules/.bin/mocha ./main.js --timeout 5000 --reporter tap 2>&1 |
	sed 's/^ /# /' |
	if type docker &>/dev/null; then
		cat
	else
		sed -E '/^(not )?ok/s/\s*#.*/ # SKIP docker is not installed/'
	fi
